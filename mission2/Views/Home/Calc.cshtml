@{
    ViewData["Title"] = "In Demand: Calculator";
}

<h2>Grade Calculator:</h2>
<form>
    <label>Assignments</label>
    <input id="assignment" type="text" /><br />

    <label>Group Project</label>
    <input id="group_project" type="text" /><br />

    <label>Quizzes</label>
    <input id="quiz" type="text" /><br />

    <label>Exams</label>
    <input id="exam" type="text" /><br />

    <label>INTEX</label>
    <input id="intex" type="text" /><br />

    <input id="grade_button" type="button" value="Calculate Grades" />
</form>
<p id="grade_output">Grade: <br />Percentage: </p>

<script src="~/js/JavaScript.js"></script>